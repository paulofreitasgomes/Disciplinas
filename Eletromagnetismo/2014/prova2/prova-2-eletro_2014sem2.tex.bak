\noindent
1) O potencial na superfície de uma esfera de raio $R$ é dado por:
\begin{equation}
V_0 (\theta) = k \cos 3 \theta = \dfrac{k}{5} \left[ 8P_3 (\cos \theta ) - 3 P_1 (\cos \theta ) \right] 
\nonumber
\end{equation}
onde $k$ é uma constante e $P_1 (\cos \theta ) = \cos \theta$ e $P_3 (\cos \theta ) = \frac{1}{2} ( 5\cos^3 \theta - 3\cos \theta )$ são os polinômios de Legendre de ordem 1 e 3. a) Mostre que o potencial em todo o espaço é: 
\begin{equation}
V (r,\theta) = \left\{
\begin{array}{rl}
 -\dfrac{3k}{5R} r P_1 (\cos \theta ) + \dfrac{8k}{5R^3} r^3 P_3 (\cos \theta ) & \text{se  } r \leq R  \\
\\
 -\dfrac{3kR^2}{5r^2}  P_1 (\cos \theta ) + \dfrac{8kR^4}{5r^4} P_3 (\cos \theta )  & \text{se  } r \geq R
\end{array} 
\right. \nonumber
\end{equation}
b) Assumindo que não há carga nem dentro nem fora da esfera, mostre que a densidade de carga na superfície é:
\begin{equation}
\sigma (\theta) = \dfrac{\varepsilon_0 k}{5R} \left[ 56 P_3 (\cos \theta )-9 P_1 (\cos \theta ) \right] \nonumber
\end{equation}

%\vspace{0.1 in}
\noindent
2) Uma carga pontual $q$ de massa $m$ é solta do repouso a uma distância $d$ de um plano infinito aterrado. Mostre que o tempo necessário para essa carga atingir o plano é:
\begin{equation}
T = \dfrac{\pi d}{q}\sqrt{2\pi \varepsilon_0 md} \nonumber
\end{equation}
\textbf{Fórmulas para consulta}

  \begin{equation}
 \vec{p} = \int {\vec{r}\,}' \rho({\vec{r}\,}') d\tau' \quad \quad V_{dip}({\vec{r}\,}') =\dfrac{\vec{p} \cdot \hat{r}}{4\pi \varepsilon_0r^2}    \quad \quad \vec{E}_{dip} (r,\theta) = \dfrac{p}{4\pi \varepsilon_0r^3}(2\hat{r}\cos \theta + \hat{\theta} \sin \theta)   \nonumber 
\end{equation} 

 \begin{equation}
 \int_a^b x^n dx = \dfrac{b^{n+1}-a^{n+1}}{n+1}  \quad \quad \vec{F} = -\dfrac{1}{4\pi \varepsilon_0} \dfrac{q^2}{(2z)^2}\hat{z} 
 \quad \quad 
 V (r,\theta) = \left\{
\begin{array}{rl}
 V_d (r,\theta) = \sum\limits_{n=0}^{\infty} A_n r^n P_n (\cos \theta) & \text{se  } r \leq R  \\
\\
V_f (r,\theta) =\sum\limits_{n=0}^{\infty} \dfrac{B_n}{r^{n+1}} P_n (\cos \theta)  & \text{se  } r \geq R
\end{array} 
\right.
 \nonumber 
\end{equation} 
%\vspace{0.01 in} 
 
\begin{equation}
  A_n = \dfrac{2n+1}{2R^n} \int_0^{\pi} V_0(\theta) P_n (\cos \theta) \sin \theta d\theta  \quad \quad V_d(R,\theta)= V_f(R,\theta) \quad \quad \sigma (\theta) = \varepsilon_0 \sum_{n=0}^{\infty} (2n+1) A_n R^{n-1} P_n (\cos \theta )   \nonumber 
\end{equation} 



\begin{equation}
 \int_0^{\pi} P_n  (\cos \theta ) P_m  (\cos \theta ) \sin \theta d\theta  = \left\{
\begin{array}{rl}
 0& \text{se  } n \neq m  \\
\\
 \dfrac{2}{2n+1}& \text{se  } n = m
\end{array} 
\right.
\quad \quad
\int_d^0 \sqrt{\dfrac{x}{d-x}}dx = -d\dfrac{\pi}{2} \quad \quad \sum_i \vec{F}_i = m\dfrac{d^2\vec{r}}{dt^2} 
 \nonumber 
\end{equation}

\begin{equation}
 \left(  \dfrac{\partial V_d }{\partial r}- \dfrac{\partial V_f }{\partial r} \right)\biggr\rvert_{r=R}   = -\dfrac{\sigma(\theta)}{\varepsilon_0}  \quad \quad \nabla^2 V(\vec{r}) = \rho(\vec{r}) \quad \quad V(\vec{r}) = \dfrac{1}{4\pi \varepsilon_0} \sum_{n=0}^{\infty} \dfrac{1}{r^{n+1}} \int (r')^n P_n (\cos \theta) \rho({\vec{r}\,}')d\tau'  \nonumber 
\end{equation} 






 
